{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Integrante</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Position</th>
                <td>{{ entity.position }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Googleaccount</th>
                <td>{{ entity.googleaccount }}</td>
            </tr>
            <tr>
                <th>Twitteraccount</th>
                <td>{{ entity.twitteraccount }}</td>
            </tr>
            <tr>
                <th>Facebookaccount</th>
                <td>{{ entity.facebookaccount }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_integrante') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_integrante_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
